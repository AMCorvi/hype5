root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 2

[*.js]
indent_style = spaces

[*.py]
indent_style = space

[*.md]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false

[package.json]
indent_style = space
