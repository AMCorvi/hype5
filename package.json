{
  "name": "hype5",
  "version": "0.0.0-development",
  "description": "Data resource for newly buzzing music using 'Hype Machine' as resource... ",
  "main": "./dist/hype5.js",
  "scripts": {
    "commit": "git-cz",
    "format": "prettier-eslint \"./src/*.js\"",
    "test": "jest --no-cache --coverage ",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AMCorvi/hype5.git"
  },
  "keywords": [
    "music"
  ],
  "author": "AMCorvi <am@corvi.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AMCorvi/hype5/issues"
  },
  "homepage": "https://github.com/AMCorvi/hype5#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-watch": "^2.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-env": "^1.6.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "ghooks": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^2.6.0",
    "jest": "^20.0.4",
    "prettier-eslint-cli": "^4.2.1",
    "regenerator-runtime": "^0.11.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "post-commit": "npm run test"
    }
  }
}
