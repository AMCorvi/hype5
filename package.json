{
  "name": "hype5",
  "version": "0.0.3",
  "description": "Data resource for newly buzzing music using 'Hype Machine' as resource... ",
  "main": "./lib/hype5.v2.js",
  "scripts": {
    "build": "babel src/hype5.v2.js src/crawler.js -d lib",
    "commit": "git-cz",
    "prestart": "eslint -- --fix",
    "start": "run-p lint:watch test:watch generatedocs:watch",
    "lint": "esw ./src/",
    "lint:watch": "npm run lint -- --watch",
    "format": "prettier-eslint \"./src/*.js\"",
    "generatedocs": "jsdoc \"./src/hlv.js\"",
    "generatedocs:watch": "nodemon --exec npm run generatedocs --watch ./src/",
    "test": "jest --no-cache --coverage ",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AMCorvi/hype5.git"
  },
  "keywords": [
    "music"
  ],
  "author": "AMCorvi <am@corvi.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AMCorvi/hype5/issues"
  },
  "homepage": "https://github.com/AMCorvi/hype5#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-watch": "^2.0.7",
    "jsdoc": "^3.5.5"
  },
  "devDependencies": {
    "@kgryte/editorconfig": "^1.0.4",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-env": "^1.6.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "docdash": "0.4.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-watch": "^3.1.3",
    "ghooks": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^2.6.0",
    "jest": "^20.0.4",
    "nodemon": "1.12.1",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.2.1",
    "regenerator-runtime": "^0.11.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "post-push": "npm run test"
    }
  }
}
